  makecachematrix <- function(p = matrix())
{
  inv <- NULL
  set <- function(q)
  {
    p <<- q
    inv <<- NULL
  }
  get <- function() p
  si <- function(sm) inv <<- sm
  gi<- function() inv
  list(set = set, get = get, si = si, gi= gi)
}

cachesolve <- function(x, ...) 
{
  in <- x$gi()
  if(!is.null(in))
  {
    message("cached data")
    return(in)
  }
  data <- x$get()
  in <- solve(data)
  x$si(in)
  in      
}
